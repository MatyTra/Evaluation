{
  "connection": "default",
  "collectionName": "restaurants",
  "info": {
    "name": "Restaurant"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nom": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "Longitude": {
      "type": "decimal",
      "unique": true
    },
    "plats": {
      "collection": "plats"
    },
    "Latitude": {
      "type": "decimal"
    },
    "id": {
      "type": "integer"
    }
  }
}
